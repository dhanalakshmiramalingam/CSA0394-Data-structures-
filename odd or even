#include<stdio.h>
int main()
{
	int num;
	printf("enter the number :");
	scanf("%d",&num );
	if(num==0)
	{
		printf("input cannot be zero");
	}
	else if (num<=0)
	{
		printf("input cannot be negative");
	}
	else if(num%2!=0)
	{
		printf("the given number is odd");
	}
	else
	{
		printf("the given number is even");
	}
}
